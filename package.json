{
  "name": "@zalaris/babylonjs-vite-template",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "tsc && vite build && vite preview --host --open",
    "lint": "biome lint --apply ./src",
    "format": "biome format --write ./src"
  },
  "typeRoots": ["node_modules/@types", "src/@types"],
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "typescript": "5.4.5",
    "vite": "5.2.10"
  },
  "dependencies": {
    "babylonjs": "7.4.0",
    "babylonjs-loaders": "7.4.0"
  }
}
